{
  "name": "@roseline124/react-tree",
  "version": "0.0.1-alpha.0",
  "private": false,
  "description": "Customizable React Tree component",
  "author": "roseline124 <guseod24@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roseline124/react-tree.git"
  },
  "bugs": {
    "url": "https://github.com/roseline124/react-tree/issues"
  },
  "homepage": "https://github.com/roseline124/react-tree#readme",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-interactions": "^8.1.0",
    "@storybook/react": "^8.1.0",
    "@storybook/test": "^8.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "semantic-release": "^24.0.0",
    "tslib": "^2.6.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
